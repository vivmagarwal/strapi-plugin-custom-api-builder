{
  "name": "strapi-plugin-custom-api",
  "version": "2.2.0",
  "description": "Create custom API endpoints from the Strapi admin panel - a visual API builder inspired by Drupal Views.",
  "strapi": {
    "name": "custom-api",
    "description": "Create custom API endpoints from the Strapi admin panel.",
    "kind": "plugin",
    "displayName": "Custom API Builder"
  },
  "exports": {
    "./strapi-admin": {
      "source": "./admin/src/index.jsx",
      "default": "./admin/src/index.jsx"
    },
    "./strapi-server": {
      "source": "./server/index.js",
      "default": "./server/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "admin",
    "server",
    "utils/helpers.js",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vivmagarwal/strapi-plugin-custom-api-builder"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@strapi/strapi": "^5.0.0"
  },
  "author": {
    "name": "Vivek M. Agarwal",
    "email": "vivmagarwal@gmail.com"
  },
  "maintainers": [
    {
      "name": "Vivek M. Agarwal",
      "email": "vivmagarwal@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/vivmagarwal/strapi-plugin-custom-api-builder/issues"
  },
  "homepage": "https://github.com/vivmagarwal/strapi-plugin-custom-api-builder#readme",
  "keywords": [
    "strapi",
    "strapi-plugin",
    "plugin",
    "api",
    "api-builder",
    "custom-api",
    "visual api builder",
    "rest api",
    "strapi v5"
  ],
  "license": "MIT"
}
